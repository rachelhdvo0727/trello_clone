rder-radius: 0.3vw;\n  font-family: var(--textFont);\n  text-align: center;\n  cursor: pointer;\n}\nbutton.addNew {\n  z-index: 999;\n  margin-top: -1vw;\n  background: url(\"assets/plus-solid.svg\") no-repeat center 100% transparent;\n  background-image: url(\"assets/plus-solid.svg\");\n  height: 2vw;\n  width: 3vw;\n}\nbutton.add {\n  background-color: yellowgreen;\n  width: 5vw;\n  align-self: flex-end;\n}\nbutton.editbtn{\n  background: url(\"assets/pencil-alt-solid.svg\") no-repeat center 100% transparent;\n  height: 1.2vw;\n  width: 1.7vw;\n}\n.hidden {\n  display: none;\n}\n.cardcontainer{\nbackground-color: #effdae;\npadding: 1vw;\nbox-shadow: var(--boxShadow2);\n}\n.btns_container{\n  display: flex;\n  flex-flow: row wrap;\n  justify-content: space-between;\n  margin-top: 0.8vw;\n}\n.movebtns{\n  /* flex child */\n  align-self:flex-end;\n   /* flex parent */\n  display: flex;\n  flex-flow: row wrap;\n  justify-content: space-around;\n}\n.close{\n  position: absolute;\n  top: 11vw;\n  left: 21vw;\n  font-size: 1.2rem;\n}\n.title:hover{\n  cursor: pointer;\n  text-decoration: underline;\n}"},"lineCount":null}},"error":null,"hash":"877f85645d26c967461ad0fd8f24e5d9","cacheData":{"env":{}}}